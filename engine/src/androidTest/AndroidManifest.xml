    <uses-permission android:name="com.oasisfeng.island.permission.LAUNCH_PACKAGE" />
    <application>
        <activity android:name="com.oasisfeng.island.api.ApiActivityTest$DummyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
